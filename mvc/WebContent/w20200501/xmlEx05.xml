<?xml version="1.0" encoding="UTF-8"?>

<!-- 속성도 DTD로 정희해줄 수 있음 -->

<!DOCTYPE 학생들 [
    <!ELEMENT 학생들 (학생)+ >
    <!ELEMENT 학생 (학번, 이름, 나이*) >
    <!ELEMENT 학번 (#PCDATA) >
    <!ELEMENT 이름 (#PCDATA) >
    <!ELEMENT 나이 (#PCDATA) >
    
    <!ATTLIST 학생 학년 (1|2|3|4) "1" >
    <!ATTLIST 학번 학과 (자바|보안|빅데이터) "자바" >
    
]> 

<학생들>
    <학생>
        <학번>20</학번>
        <이름>홍길동</이름>
        <나이>19</나이>
    </학생>
    <학생 학년="2">
        <학번 학과="보안">19</학번>
        <이름>유관순</이름>
        <나이>20</나이>
    </학생>




</학생들>